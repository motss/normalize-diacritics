{
  "name": "normalize-diacritics",
  "version": "2.13.2",
  "description": "Remove accents/ diacritics in string",
  "keywords": [
    "diacritic",
    "diacritics",
    "motss",
    "normalize",
    "normalize-diacritics",
    "npm"
  ],
  "homepage": "https://github.com/motss/normalize-diacritics",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/normalize-diacritics.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng (motss)",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "node": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./dist/*": "./dist/*",
    "./normalize-diacritics": "./dist/normalize-diacritics.js"
  },
  "main": "./dist/index",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/*.*js.map",
    "dist/*.*js",
    "dist/*.d.ts.map",
    "dist/*.d.ts"
  ],
  "scripts": {
    "build": "npm run clean && npm run rollup",
    "check": "package-check",
    "clean": "shx rm -rf .*cache *.log .tmp coverage/ dist/ logs/",
    "postinstall": "npm dedupe",
    "lint": "eslint",
    "lint:build": "npm run lint -- --config .build.eslintrc.json",
    "prepublishOnly": "npm run check && npm run lint && npm run build",
    "rollup": "rollup -c ./rollup.config.js",
    "test": "c8 --reporter=lcov --exclude=src/tests uvu -r @swc/register src/tests",
    "test:watch": "npm run clean && watchlist src -- npm t",
    "ts": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm x -- husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check && npm run lint && tsc --noEmit"
    }
  },
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@reallyland/tsconfig": "^3.0.2",
    "@skypack/package-check": "^0.2.2",
    "@swc/register": "^0.1.7",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "c8": "^7.7.3",
    "eslint": "^7.17.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-regexp": "^0.12.1",
    "rollup": "^2.29.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "shx": "^0.3.2",
    "typescript": "^4.1.3",
    "uvu": "^0.5.1",
    "watchlist": "^0.2.3"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
